#include <stdio.h>

int main() {
    int a = 10, b = 4;
    int sum, difference, product, quotient, remainder;

    // Performing arithmetic operations
    sum = a + b;
    difference = a - b;
    product = a * b;
    quotient = a / b;
    remainder = a % b;

    // Displaying the results
    printf("a + b = %d\n", sum);
    printf("a - b = %d\n", difference);
    printf("a * b = %d\n", product);
    printf("a / b = %d\n", quotient);
    printf("a %% b = %d\n", remainder);

    return 0;
}
